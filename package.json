{
  "name": "webssh2",
  "version": "0.2.3",
  "ignore": [
    ".gitignore"
  ],
  "bin": "./index.js",
  "description": "A Websocket to SSH2 gateway using term.js, socket.io, ssh2, and express",
  "homepage": "https://github.com/billchurch/WebSSH2",
  "keywords": "ssh webssh terminal webterminal",
  "license": "SEE LICENSE IN FILE - LICENSE",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/billchurch/WebSSH2.git"
  },
  "contributors": [
    {
      "name": "Bill Church",
      "email": "wmchurch@gmail.com"
    }
  ],
  "engines": {
    "node": "6"
  },
  "bugs": {
    "url": "https://github.com/billchurch/WebSSH2/issues"
  },
  "dependencies": {
    "basic-auth": "^2.0.0",
    "colors": "^1.1.2",
    "compression": "^1.7.1",
    "debug": "^3.1.0",
    "express": "^4.16.1",
    "express-session": "^1.15.6",
    "html-webpack-plugin": "^2.30.1",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "read-config": "^2.0.0",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.1.0",
    "ssh2": "^0.6.1",
    "validator": "^10.1.0"
  },
  "scripts": {
    "start": "node index.js",
    "build": "webpack --progress --colors --config scripts/webpack.prod.js",
    "builddev": "webpack --progress --colors --config scripts/webpack.dev.js --mode development",
    "dev": "webpack --progress --colors --watch --config scripts/webpack.dev.js --mode development",
    "test": "snyk test",
    "watch": "nodemon index.js",
    "standard": "standard --verbose | snazzy",
    "cleanmac": "find . -name '.DS_Store' -type f -delete"
  },
  "devDependencies": {
    "ajv": "^6.5.0",
    "bithound": "^1.7.0",
    "clean-webpack-plugin": "^0.1.18",
    "css-loader": "^0.28.9",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^1.1.6",
    "nodemon": "^1.11.0",
    "postcss-discard-comments": "^2.0.4",
    "snazzy": "^7.0.0",
    "snyk": "^1.39.1",
    "standard": "^11.0.0",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^1.12.15",
    "webpack-merge": "^4.1.2",
    "xterm": "^3.1.0"
  },
  "standard": {
    "ignore": [
      "client/public/webssh2.bundle.js",
      "bigip/*",
      "screenshots/*",
      "bin/*",
      "build/*"
    ]
  }
